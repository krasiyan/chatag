{
  "name": "chatag",
  "version": "1.0.0",
  "main": "server/server.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "postinstall": "if test \"$IN_BUILD_PIPELINE\" != \"1\" ; then cd client && npm i ; fi",
    "db:seed": "node ./scripts/seed.js",
    "dev": "run-p -l dev:*",
    "dev:server": "backpack dev",
    "dev:client": "npm start --prefix client",
    "build": "run-p -l build:*",
    "prebuild:client": "rimraf build/client",
    "build:client": "npm run build --prefix client",
    "postbuild:client": "cp -r client/build build/client",
    "prebuild:server": "rimraf build/server",
    "build:server": "backpack build",
    "postbuild:server": "cp -t build/server package.json .npmrc && cd build/server && IN_BUILD_PIPELINE=1 npm i",
    "test": "run-s -l test:*",
    "test:lint": "eslint .",
    "test:client": "npm test --prefix client",
    "test:posttest": "npm run test:lint && nsp check"
  },
  "pre-commit": "test:lint",
  "dependencies": {
    "async": "2.6.0",
    "compression": "1.7.2",
    "cors": "2.8.4",
    "dotenv": "5.0.1",
    "helmet": "3.12.0",
    "loopback": "3.18.3",
    "loopback-boot": "2.27.1",
    "loopback-component-explorer": "5.3.0",
    "loopback-connector-mongodb": "3.4.1",
    "serve-favicon": "2.4.5",
    "socket.io": "2.0.4",
    "socket.io-emitter": "3.1.1",
    "socket.io-redis": "5.2.0",
    "strong-error-handler": "2.3.2"
  },
  "devDependencies": {
    "backpack-core": "0.7.0",
    "eslint": "4.19.1",
    "eslint-config-loopback": "10.0.0",
    "json-loader": "0.5.7",
    "npm-run-all": "4.1.2",
    "nsp": "3.2.1",
    "pre-commit": "1.2.2",
    "rimraf": "2.6.2",
    "temp": "0.8.3"
  },
  "repository": {
    "type": "",
    "url": ""
  },
  "license": "UNLICENSED",
  "description": "chatag"
}
