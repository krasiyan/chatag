{
  "name": "chatag",
  "version": "1.0.0",
  "main": "server/server.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "postinstall": "cd client && npm i",
    "seed": "node ./scripts/seed.js",
    "develop": "run-p -l develop:*",
    "develop:server": "nodemon .",
    "develop:client": "npm start --prefix client",
    "build:client": "npm run build --prefix client",
    "test": "run-s -l test:*",
    "test:lint": "eslint .",
    "test:client": "npm test --prefix client",
    "test:posttest": "npm run test:lint && nsp check"
  },
  "dependencies": {
    "compression": "^1.0.3",
    "cors": "^2.5.2",
    "dotenv": "5.0.1",
    "helmet": "^3.10.0",
    "loopback": "^3.0.0",
    "loopback-boot": "^2.6.5",
    "loopback-component-explorer": "^5.0.0",
    "loopback-connector-mongodb": "1.18.1",
    "serve-favicon": "^2.0.1",
    "socket.io": "2.0.4",
    "socket.io-redis": "5.2.0",
    "strong-error-handler": "^2.0.0"
  },
  "devDependencies": {
    "eslint": "^3.17.1",
    "eslint-config-loopback": "^8.0.0",
    "nodemon": "1.17.1",
    "npm-run-all": "4.1.2",
    "nsp": "^2.1.0"
  },
